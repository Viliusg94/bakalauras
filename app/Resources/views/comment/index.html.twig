        <br>

        <h1 id="komentarai">JŪSŲ KOMENTARAS</h1>
        {{ form_start(form) }}
        {{ form_errors(form) }}

        {# The two following syntaxes are equivalent #}
        <div class="form-group">
            {{ form_label(form.name, 'Jūsų vardas') }}
            {{ form_widget(form.name, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <div  class="container">
            {{ form_label(form.text, 'Atsiliepimas') }}
            {{ form_widget(form.text, { 'attr': {'class': 'form-control'} }) }}
        </div>


        <input type="submit" value="Komentuoti" />

        {{ form_end(form) }}


        <h1>KOMENTARAI</h1>

        <table>
            <thead>
            <tr>

                <th>Vardas</th>
                <th>Komentaras</th>
                <th>Veiksmai</th>
            </tr>
            </thead>
            <tbody>
            {% for comment in comments %}
                <tr>
                    {#<td><a href="{{ path('_show', { 'id': comment.id }) }}">{{ comment.id }}</a></td>#}
                    <td>{{ comment.name }}</td>
                    <td>{{ comment.text }}</td>
                    {#<td>{% if comment.created %}{{ comment.created|date('Y-m-d H:i:s') }}{% endif %}</td>#}
                    <td>
                        <ul>
                            {#<li>#}
                                {#<a href="{{ path('_show', { 'id': comment.id }) }}">show</a>#}
                            {#</li>#}
                            <li>
                                <a href="{{ path('_edit', { 'id': comment.id }) }}">Redaguoti</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>

        {#<ul>#}
