{% extends 'base.html.twig' %}
{% block header %}
    <title>Skandinavijos Kurjeris</title>
    <link href="css/bootstrap.css" rel='stylesheet' type='text/css' />
    <script src="js/jquery.min.js"></script>
    {#<link href="css/style.css" rel="stylesheet" type="text/css" media="all" />#}
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Skandinavijos kurjeris, pervezimai, svedija, norvegija" />

    <!--sriftas-->
    <link href="css/owl.carousel.css" rel="stylesheet">

    <link href='//fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Raleway:400,800,300,100,500,700,600,900' rel='stylesheet' type='text/css'>
    <!--animacija css-->
    <link href="css/animate.css" rel="stylesheet" type="text/css" media="all">
    <script src="js/wow.min.js"></script>
    <!--animacija css-->

    <!--scriptas-->
    <script type="text/javascript" src="js/move-top.js"></script>
    <script type="text/javascript" src="js/easing.js"></script>

    <link rel="stylesheet" href="css/swipebox.css">
    <script src="js/jquery.swipebox.min.js"></script>

{% endblock %}
{% block body %}
    <h1>KOMENTARŲ REDAGAVIMAS</h1>

    {{ form_start(edit_form) }}
        {{ form_widget(edit_form) }}
        <input type="submit" value="Redaguoti" />
    {{ form_end(edit_form) }}

    <ul>
        <li>
            <a href="{{ path('_index') }}">Grįžti</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Ištrinti">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
